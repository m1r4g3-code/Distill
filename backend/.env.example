# Database
DATABASE_URL=postgresql+asyncpg://postgres:dev@localhost:5432/webextract
SECRET_KEY=change-me-32-chars-minimum

# Redis
REDIS_URL=redis://localhost:6379/0

# Cache & Fetch
CACHE_TTL_SECONDS=3600
FETCH_CONNECT_TIMEOUT=5
FETCH_READ_TIMEOUT=20

# Playwright
PLAYWRIGHT_TIMEOUT=30

# Rate Limiting
DOMAIN_DELAY_MS=500
DEFAULT_RATE_LIMIT=60

# Proxy Settings
PROXY_ENABLED=False
PROXY_URL=http://your-proxy-url:port
PROXY_ROTATE=False

# Job Cleanup
JOB_CLEANUP_INTERVAL_HOURS=1

# Search Providers (use one or both)
SERPER_API_KEY=your-serper-key
SERPAPI_API_KEY=your-serpapi-key
SEARCH_TIMEOUT=10

# Gemini
GEMINI_API_KEY=your-gemini-key

# Admin Key for managing API Keys via /api/v1/admin
ADMIN_KEY=change-me-admin-key

# Supabase Auth Integration (required for User API Key sync)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=ey...

# Monitoring
SENTRY_DSN=

# Environment
LOG_LEVEL=INFO
APP_ENV=development
APP_VERSION=1.3.1
ENVIRONMENT=production
